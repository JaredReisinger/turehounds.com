{
  "name": "turehounds.com",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "Jared Reisinger",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf _site",
    "build": "npm-run-all --print-label css eleventy:prod search:build titles:build",
    "build:dev": "npm-run-all --print-label css eleventy search:build titles:build",
    "start": "npm-run-all --print-label --parallel css eleventy",
    "dev": "npm-run-all --print-label build:dev --parallel css:dev eleventy:dev",
    "css": "postcss src/static/css/tailwind.css --o _site/static/css/style.css",
    "css:dev": "npm run css -- --watch",
    "eleventy": "eleventy",
    "eleventy:prod": "cross-env NODE_ENV=production npm run eleventy",
    "eleventy:dev": "npm run eleventy -- --serve --watch",
    "eleventy:debug": "cross-env DEBUG=Eleventy* npm run eleventy -- --dryrun",
    "search:build": "node src/_helpers/build_search_index.js",
    "search:test": "node src/_helpers/test_search_index.js",
    "titles:build": "node src/_helpers/build_title_map.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@shopify/prettier-plugin-liquid": "^1.2.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "alpinejs": "^3.12.3",
    "autoprefixer": "^10.4.14",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "fontawesome-free": "^1.0.4",
    "glob": "^10.3.3",
    "heroicons": "^2.0.18",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lunr": "^2.3.9",
    "luxon": "^3.3.0",
    "node-window-polyfill": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.3"
  }
}
