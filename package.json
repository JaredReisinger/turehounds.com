{
  "name": "turehounds.com",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "Jared Reisinger",
  "license": "ISC",
  "scripts": {
    "postinstall": "patch-package",
    "clean": "rimraf _js _site @types/@11ty",
    "prebuild": "npm-run-all -l :prebuild:11ty-types -p :prebuild:code :prebuild:data",
    ":prebuild:11ty-types": "tsc --project tsconfig.11ty.json 1> /dev/null || true",
    ":prebuild:code": "tsc --build",
    ":prebuild:data": "npm-run-all -l -p :prebuild:data:*",
    ":prebuild:data:plugins": "cpy --cwd . --dot 'plugins/**/*' '!**/*.{js,ts}' './_js/plugins'",
    ":prebuild:data:src": "cpy --cwd . --dot 'src/**/*' '!**/*.{js,ts}' './_js/src'",
    ":prebuild:data:tools": "cpy --cwd . --dot 'tools/**/*' '!**/*.{js,ts}' './_js/tools'",
    "build": "npm-run-all -l css eleventy:prod build:data",
    "build:data": "npm-run-all -l -p search:build titles:build",
    "build:dev": "npm-run-all -l css eleventy build:data",
    "prebuild:dev": "npm run prebuild",
    "start": "npm-run-all -l --parallel css eleventy",
    "dev": "npm-run-all -l build:dev --parallel css:dev src:dev eleventy:dev",
    "src:dev": "monex --watch 'src' --ignore '**/*.{js,ts}' --delay 250 --exec 'npm::prebuild:data:src'",
    "css": "postcss _js/src/static/css/tailwind.css --o _site/static/css/style.css",
    "css:dev": "npm run css -- --watch",
    "eleventy": "eleventy --config ./_js/.eleventy.js",
    "eleventy:prod": "cross-env NODE_ENV=production npm run eleventy",
    "eleventy:dev": "npm run eleventy -- --serve --watch --ignore-initial",
    "eleventy:debug": "cross-env DEBUG=Eleventy* npm run eleventy -- --dryrun",
    "preeleventy:debug": "npm run prebuild",
    "search:build": "node _js/tools/build_search_index.js",
    "search:test": "node _js/tools/test_search_index.js",
    "titles:build": "node _js/tools/build_title_map.js",
    "titles:test": "npm run prebuild && node _js/tools/test_title_map.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@shopify/prettier-plugin-liquid": "^1.5.0",
    "@tailwindcss/forms": "^0.5.8",
    "@tailwindcss/typography": "^0.5.15",
    "@types/11ty__eleventy-img": "^4.0.0",
    "@types/debug": "^4.1.12",
    "@types/html-minifier": "^4.0.5",
    "@types/js-yaml": "^4.0.9",
    "@types/lunr": "^2.3.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.2",
    "alpinejs": "^3.14.1",
    "autoprefixer": "^10.4.20",
    "cheerio": "^1.0.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.5",
    "debug": "^4.3.6",
    "exif-reader": "^2.0.1",
    "fontawesome-free": "^1.0.4",
    "glob": "^11.0.0",
    "heroicons": "^2.1.5",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "luxon": "^3.5.0",
    "monex": "^2.2.1",
    "node-window-polyfill": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "photoswipe": "^5.4.4",
    "photoswipe-dynamic-caption-plugin": "^1.2.7",
    "postcss": "^8.4.43",
    "postcss-cli": "^11.0.0",
    "postcss-nested": "^6.2.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "watcher": "^2.3.1"
  }
}
